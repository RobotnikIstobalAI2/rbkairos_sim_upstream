        <arg name="load_arm_kinematics_file" value="$(arg load_arm_kinematics_file)"/>
        <arg name="launch_base_hw_sim" value="$(arg launch_base_hw_sim)"/>
        <arg name="launch_battery_estimator" value="$(arg launch_battery_estimator)"/>
