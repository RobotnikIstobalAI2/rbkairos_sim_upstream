    <arg name="gmapping_robot_c" default="false"/>
    <arg name="move_base_robot_c" default="true"/>
    <arg name="amcl_and_mapserver_c" default="true"/>
    <arg name="launch_pad_c" default="true"/>
    <!-- spawn position robot c -->
    <arg name="x_init_pose_robot_c" default="2" />
    <arg name="y_init_pose_robot_c" default="2" />
    <arg name="z_init_pose_robot_c" default="0" />
    <arg name="yaw_init_pose_robot_c" default="0" />
    <!-- arm robot c -->
    <arg name="launch_arm_c" default="true"/>
    <arg name="arm_model_c" default="$(arg default_arm_model)"/>
    <arg name="moveit_movegroup_c" default="false"/>
    <!-- gripper robot c -->
    <arg name="launch_gripper_c" default="true"/>
    <arg name="gripper_model_c" default="$(arg default_gripper_model)"/>
    <!-- map_frame robot c -->
    <arg name="map_frame_robot_c" default="$(arg map_frame)"/>
    
    <!-- arguments robot d -->
    <arg name="id_robot_d" default="robot_d"/>
    <arg name="launch_robot_d" default="false"/>
    <arg name="xacro_robot_d" default="$(arg default_xacro)"/>
    <arg name="map_package_d" value="$(arg map_package)"/>
    <arg name="map_folder_d" value="$(arg map_folder)"/>
    <arg name="map_file_d" value="$(arg map_file)"/>
    <arg name="localization_robot_d" default="false"/>
    <arg name="gmapping_robot_d" default="false"/>
    <arg name="move_base_robot_d" default="true"/>
    <arg name="amcl_and_mapserver_d" default="true"/>
    <arg name="launch_pad_d" default="true"/>
    <!-- spawn position robot d -->
    <arg name="x_init_pose_robot_d" default="-1" />
    <arg name="y_init_pose_robot_d" default="1" />
    <arg name="z_init_pose_robot_d" default="0" />
    <arg name="yaw_init_pose_robot_d" default="0" />
    <!-- arm robot d -->
    <arg name="launch_arm_d" default="true"/>
    <arg name="arm_model_d" default="$(arg default_arm_model)"/>
    <arg name="moveit_movegroup_d" default="false"/>
    <!-- gripper robot d -->
    <arg name="launch_gripper_d" default="true"/>
    <arg name="gripper_model_d" default="$(arg default_gripper_model)"/>
    <!-- map_frame robot d -->
    <arg name="map_frame_robot_d" default="$(arg map_frame)"/>

   <!-- arguments robot e -->
    <arg name="id_robot_e" default="robot_e"/>
    <arg name="launch_robot_e" default="false"/>
    <arg name="xacro_robot_e" default="$(arg default_xacro)"/>
    <arg name="map_package_e" value="$(arg map_package)"/>
    <arg name="map_folder_e" value="$(arg map_folder)"/>
    <arg name="map_file_e" value="$(arg map_file)"/>
    <arg name="localization_robot_e" default="false"/>
    <arg name="gmapping_robot_e" default="false"/>
    <arg name="move_base_robot_e" default="true"/>
    <arg name="amcl_and_mapserver_e" default="true"/>
