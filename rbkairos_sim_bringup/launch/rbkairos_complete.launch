        <arg name="id_robot_d" value="$(arg id_robot_d)"/>
        <arg name="launch_robot_d" value="$(arg launch_robot_d)"/>
        <arg name="xacro_robot_d" value="$(arg xacro_robot_d)"/>
        <arg name="map_package_d" value="$(arg map_package_d)"/>
        <arg name="map_folder_d" value="$(arg map_folder_d)"/>
        <arg name="map_file_d" value="$(arg map_file_d)"/>
        <arg name="localization_robot_d" value="$(arg localization_robot_d)"/>
        <arg name="gmapping_robot_d" value="$(arg gmapping_robot_d)"/>
